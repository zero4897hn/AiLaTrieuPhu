<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zero.ailatrieuphu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiemCaoActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".TuyChinhActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LuatChoiActivity"
            android:noHistory="true"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".CauHoiActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".LuatChoiTiepTheoActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>